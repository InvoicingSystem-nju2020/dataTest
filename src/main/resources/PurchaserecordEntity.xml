<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="com.example.demo.entity.PurchaserecordEntity">
        <table name="purchaserecord" schema="invoicingsystem" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id"/>
            </id>
            <basic name="createTime">
                <column name="createTime"/>
            </basic>
            <basic name="purchaseTime">
                <column name="purchaseTime"/>
            </basic>
            <basic name="goodsNumber">
                <column name="goodsNumber"/>
            </basic>
            <basic name="numbers">
                <column name="numbers"/>
            </basic>
            <basic name="dicount">
                <column name="dicount"/>
            </basic>
            <basic name="unitPrice">
                <column name="unitPrice"/>
            </basic>
            <basic name="balance">
                <column name="balance"/>
            </basic>
            <basic name="taxIncluded">
                <column name="taxIncluded"/>
            </basic>
            <basic name="precautionsForPreservation">
                <column name="precautionsForPreservation"/>
            </basic>
            <basic name="buyer">
                <column name="buyer"/>
            </basic>
            <basic name="remarks">
                <column name="remarks"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
